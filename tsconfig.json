{
    "compilerOptions": {
        "module": "commonjs",
        "declaration": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "target": "es2017",
        "sourceMap": true,
        "outDir": "./dist",
        "baseUrl": "./",
        "skipLibCheck": true,
        "strictNullChecks": false,
        "forceConsistentCasingInFileNames": false,
        "strict": true,
        "esModuleInterop": true,
        "resolveJsonModule": true
    }
}
